/home/mitsudome-r/.ros/log/latest/*stderr.log
/home/mitsudome-r/.ros/log/latest/*stdout.log
{
    size 10M
    su mitsudome-r mitsudome-r
    rotate 10
    missingok
    notifempty
    compress
    delaycompress
    copytruncate
}

/home/mitsudome-r/.ros/log/log.tar.gz
{
    su mitsudome-r mitsudome-r
    copy
    daily
    rotate 10
    missingok
    notifempty
    nocompress
}

